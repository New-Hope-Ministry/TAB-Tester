    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Node afixMin.js",
            "program": "${workspaceFolder}/data/afixMin.js",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Edge Launch synth.html",
            "url": "http://localhost:5500/synth.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch chron.html",
            "url": "http://localhost:5500/chron.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch synth.html",
            "url": "http://localhost:5500/synth.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch index.html",
            "url": "http://localhost:5500/index.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}